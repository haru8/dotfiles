alias ls='ls -CFh --color=auto'
alias du='du -h'
alias df='df -h'
alias cp='cp -i'
alias mv='mv -i'
alias rm='rm -i'
alias crontab='crontab -i'
alias c='clear'
alias j='jobs'
alias h='history'
alias less='less -iM -j5 -x4 -X'
alias grepc='grep --color=always'
alias ctags='ctags -R --langmap=PHP:.php.inc --php-types=cfdv'

